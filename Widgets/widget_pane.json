{
  "__all__": {
    "extensions": [".ovf", ".omf", ".odt", ".txt"]
  },
  "Canvas": {
    "alias": "2D Matplotlib",
    "object": "Canvas",
    "object_type": "stateful",
    "extensions": [".txt", ".odt"],
    "required": [
      "directory",
      "iterations",
      "current_state",
      "options",
      "plot_data",
      "screenshot_dir"
    ],
    "optional": ["trigger"],
    "iterable_type": "plot",
    "toolbar": "NavigationToolbar"
  },
  "Canvas2Dupgraded": {
    "alias": "2D PyQT",
    "object": "Canvas2Dupgraded",
    "object_type": "stateful",
    "extensions": [".txt", ".odt"],
    "settings": ["PlotSettings", "plot_data"],
    "required": [
      "iterations",
      "current_state",
      "options",
      "plot_data",
      "geom",
      "screenshot_dir"
    ],
    "optional": ["trigger"],
    "iterable_type": "plot",
    "toolbar": null
  },
  "CanvasLayer": {
    "alias": "2D Layer",
    "object": "CanvasLayer",
    "object_type": "stateful",
    "extensions": [".txt", ".odt"],
    "settings": ["SimplePerfOptions", "file_header"],
    "required": [
      "current_state",
      "options",
      "file_header",
      "color_vectors",
      "screenshot_dir"
    ],
    "optional": ["iterations"],
    "iterable_type": "structure",
    "toolbar": "NavigationToolbar"
  },
  "StateMachine": {
    "alias": "StateMachine",
    "object": "StateMachine",
    "object_type": "stateful",
    "extensions": [".ovf", ".omf"],
    "required": ["directory", "current_state", "screenshot_dir", "geom"],
    "optional": ["iterations", "trigger"],
    "iterable_type": "structure",
    "toolbar": [
      ["Reset", "initial_transformation"],
      ["Zoom in", "zoomIn"],
      ["Zoom out", "zoomOut"],
      ["Screenshot", "screenshot_manager"],
      ["Text", "popup_text_selector"]
    ]
  }
}
