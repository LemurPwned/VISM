                                   "screenshot_dir", "geom"],
                      "optional": ["iterations", "trigger"],
                      "iterable_type": "structure",
                      "toolbar" : [["Reset", "initial_transformation"],
                                                   ["Zoom in", "zoomIn"],
                                                   ["Zoom out", "zoomOut"],
                                                   ["Screenshot", "screenshot_manager"],
                                                   ["Text", "popup_text_selector"]]
                      },
    "VectorGLContext": {
                      "alias": "3D VECTOR",
